<Page x:Class="Autotest.Wpf.Pages.TicketsPage"
      xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
      xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
      xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
      xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
      xmlns:local="clr-namespace:Autotest.Wpf.Pages"
      mc:Ignorable="d" 
      d:DesignHeight="450" d:DesignWidth="800"
      Title="TicketsPage" Background="AntiqueWhite">

    <Grid>
        <Label VerticalAlignment="Top" HorizontalAlignment="Center" Height="60" FontSize="30" Content="Tickets"></Label>
        <ScrollViewer Margin="0,60,0,60">
            <StackPanel Name="TicketButtonsPanel">
            </StackPanel>
        </ScrollViewer>
        <Button Click="MenuButtonClick" HorizontalAlignment="Right" VerticalAlignment="Bottom"  Width="100" Height="60" Content="Menu"></Button>
    </Grid>
</Page>